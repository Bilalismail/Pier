<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.groupl.project.pier">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.groupl.project.pier.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.groupl.project.pier.ActivityOne"></activity>
        <activity android:name="com.groupl.project.pier.ActivityTwo"></activity>
        <activity android:name="com.groupl.project.pier.ActivityThree"></activity>

        <activity
            android:name="com.groupl.project.pier.Login"
            android:excludeFromRecents="true"
            android:theme="@style/NoActionBar" />
        <activity
            android:name="com.groupl.project.pier.settingPage"
            android:excludeFromRecents="true"
            android:theme="@style/NoActionBar" />
        <activity
            android:name="com.groupl.project.pier.aboutUS"
            android:excludeFromRecents="true"
            android:theme="@style/NoActionBar" />
    </application>

</manifest>